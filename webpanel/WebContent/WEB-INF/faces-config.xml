<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">
	<application>
		<message-bundle>xdisk.messagesResource</message-bundle>
	</application>
	<managed-bean>
		<managed-bean-name>userBean</managed-bean-name>
		<managed-bean-class>xdisk.UserBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>utilizzato per la registrazione di nuovi utenti</description>
		<managed-bean-name>newUserBean</managed-bean-name>
		<managed-bean-class>xdisk.persistence.User</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dataTable</managed-bean-name>
		<managed-bean-class>xdisk.DataTable</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
	 	<managed-bean-name>FolderBean</managed-bean-name>
	 	<managed-bean-class>xdisk.FolderBean</managed-bean-class>
	 	<managed-bean-scope>request</managed-bean-scope>
	 </managed-bean>
	<managed-bean>
	 	<managed-bean-name>itemFolders</managed-bean-name>
	 	<managed-bean-class>xdisk.ItemFolders</managed-bean-class>
	 	<managed-bean-scope>application</managed-bean-scope>
	 </managed-bean>
	<managed-bean>
	 	<managed-bean-name>folder</managed-bean-name>
	 	<managed-bean-class>xdisk.persistence.Folder</managed-bean-class>
	 	<managed-bean-scope>request</managed-bean-scope>
	 </managed-bean>
	<managed-bean>
		<managed-bean-name>actionAddNew</managed-bean-name>
		<managed-bean-class>xdisk.ActionAddNew</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actionGeneric</managed-bean-name>
		<managed-bean-class>xdisk.ActionGeneric</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>itemUser</managed-bean-name>
		<managed-bean-class>xdisk.ItemUser</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>itemExtension</managed-bean-name>
		<managed-bean-class>xdisk.ItemExtension</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>extension</managed-bean-name>
		<managed-bean-class>xdisk.persistence.Extension</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>itemFile</managed-bean-name>
		<managed-bean-class>xdisk.ItemFile</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>file</managed-bean-name>
		<managed-bean-class>xdisk.persistence.File</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>index</display-name>
		<from-view-id>/index.jsp</from-view-id>
		<navigation-case>
			<from-outcome>loginAdmin</from-outcome>
			<to-view-id>/admin.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>home</display-name>
		<from-view-id>/home.jsp</from-view-id>
		<navigation-case>
			<to-view-id>/index.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>newUser</from-outcome>
			<to-view-id>/addUser.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>newFolder</from-outcome>
			<to-view-id>/addFolder.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>listFolder</from-outcome>
			<to-view-id>/listFolder.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>listUser</from-outcome>
			<to-view-id>/listUser.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>return</from-outcome>
			<to-view-id>/admin.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>personal</from-outcome>
			<to-view-id>/personal.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>deleteFolder</from-outcome>
			<to-view-id>/delFolder.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>deleteUser</from-outcome>
			<to-view-id>/delUser.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>extension</from-outcome>
			<to-view-id>/addDelExtension.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>listExtension</from-outcome>
			<to-view-id>/listExtension.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>listFile</from-outcome>
			<to-view-id>/listFile.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>file</from-outcome>
			<to-view-id>/addDelFile.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index</display-name>
		<from-view-id>/index.jsp</from-view-id>
		<navigation-case>
			<from-outcome>loginClient</from-outcome>
			<to-view-id>/client.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
